# GraphQL schema example
#
# https://gqlgen.com/getting-started/

scalar Time

#//user_id, acc_id, email, department, created_at
type User {
  user_id: String!
  acc_id: String!
  email: String!
  department: String!
  created_at: Time!
}

type Query {
  users: [User!]!
  getUserByEmailDomain(domain: String!): [User!]!
}
